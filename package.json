{
  "name": "app-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "cp .env.development .env && expo start",
    "start:prod": "cp .env.production .env && expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android:dev": "cp .env.development .env && expo start --android",
    "android:prod": "cp .env.production .env && expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "cp .env.development .env && expo start --ios",
    "ios:prod": "cp .env.production .env && expo start --ios",
    "web": "expo start --web",
    "web:dev": "cp .env.development .env && expo start --web",
    "web:staging": "cp .env.staging .env && expo start --web",
    "web:prod": "cp .env.production .env && expo start --web",
    "build:dev": "cp .env.development .env && eas build --profile development",
    "build:prod": "cp .env.production .env && eas build --profile production",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "dotenv": "^17.2.2",
    "expo": "~54.0.10",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "tsc-files": "^1.1.4",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": "tsc-files --noEmit"
  },
  "private": true
}
